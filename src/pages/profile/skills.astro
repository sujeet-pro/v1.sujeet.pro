---
import Layout from "../../layouts/layout.astro";
import TechSkills from "../../components/profile/tech-skills.astro";
import { siteProfile } from "../../constants/links";
import Breadcrumb from "../../components/breadcrumb.astro";

const title = "Sujeet's Skills";
const description =
  "JS, TS, HTML, CSS, React, Gatsby, Next.JS, Angular, Astro, Vue.js and much more.";
---

<Layout
  article={false}
  title={title}
  description={description}
  image={null}
  pageDetails={{ type: "site" }}
>
  <Breadcrumb links={[siteProfile]} currentPageTitle="Skills" />
  <TechSkills />
</Layout>
